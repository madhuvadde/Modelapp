<table border='1'>
	<thead>
		<tr>
			<th>Material name</th>
			<th>Download link</th>
			<th>Action</th>
		</tr>
	</thead>
		<tbody>
			<% @post.each do |p| %>
			<tr>
			<td><%= p.name %></td>
			<td><%= link_to 'Download', p.attachment_url %></td>
			<td><%= link_to 'Delete', p, method: :delete, confirm: "Are you sure to delete #{p.name}?" %></td>
			</tr>
			</tbody>
			<% end %>
		</tbody>
		<br>
		<h4><%= link_to "New Post",new_post_path %> </h4>
	</table>